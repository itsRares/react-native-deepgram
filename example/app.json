{
  "expo": {
    "name": "React Native Deepgram Example",
    "slug": "react-native-deepgram-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android"
    ],
    "jsEngine": "hermes",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.ahhzo.react-native-deepgram-example"
    },
    "plugins": [
      [
        "react-native-deepgram",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone"
        }
      ]
    ],
  }
}
